{
  "manifest_version": 3,
  "name": "Code Executor",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "js_icon_16.png"
  },
  "icons": {
    "16": "js_icon_16.png",
    "32": "js_icon_32.png",
    "64": "js_icon_64.png"
  },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        }
      }
    }
}